    "#양수+1,음수+1\n",
    "def ad1(num=int,add=int):\n",
    "    \"\"\"'num'이 양수면 'add'를 더합니다\n",
    "       'num'이 음수면 add를 뺍니다\"\"\"\n",
    "    if num == abs(num):\n",
    "        return num+add\n",
    "    else:\n",
    "        return num-add"
   ]
  },
